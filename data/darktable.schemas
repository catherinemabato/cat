<gconfschemafile>
  <schemalist>
    <schema>
      <key>/schemas/apps/darktable/bauhaus/scale</key>
      <applyto>/apps/darktable/bauhaus/scale</applyto>
      <owner>darktable</owner>
      <type>float</type>
      <default>1.4</default>
      <gettext_domain>darktable</gettext_domain>
      <locale name="C">
        <short>widget scale</short>
        <long>scaling factor for bauhaus widgets, will affect font size</long>
      </locale>
    </schema>

    <schema><!--tab:core-->
      <key>/schemas/apps/darktable/database</key>
      <applyto>/apps/darktable/database</applyto>
      <owner>darktable</owner>
      <type>string</type>
      <default>library.db</default>
      <gettext_domain>darktable</gettext_domain>
      <locale name="C">
        <short>database location</short>
        <long>filename relative to ~/.config/darktable or starting with a slash (needs a restart).</long>
      </locale>
    </schema>

    <schema><!--tab:gui-->
      <key>/schemas/apps/darktable/panel_width</key>
      <applyto>/apps/darktable/panel_width</applyto>
      <owner>darktable</owner>
      <type>int</type>
      <default>300</default>
      <gettext_domain>darktable</gettext_domain>
      <locale name="C">
        <short>width of the side panels in pixels</short>
        <long> (needs a restart)</long>
      </locale>
    </schema>

    <schema><!--tab:core-->
      <key>/schemas/apps/darktable/cache_memory</key>
      <applyto>/apps/darktable/cache_memory</applyto>
      <owner>darktable</owner>
      <type>int</type>
      <default>536870912</default>
      <gettext_domain>darktable</gettext_domain>
      <locale name="C">
        <short>memory in bytes to use for mipmap cache</short>
        <long> (needs a restart) </long>
      </locale>
    </schema>

    <schema><!--tab:core-->
      <key>/schemas/apps/darktable/worker_threads</key>
      <applyto>/apps/darktable/worker_threads</applyto>
      <owner>darktable</owner>
      <type>int</type>
      <default>2</default>
      <gettext_domain>darktable</gettext_domain>
      <locale name="C">
        <short>number of background threads</short>
        <long>this controls for example how many threads are used to create thumbnails during import. the cache will grow to a maximum of twice this number of full resolution image buffers (needs a restart).</long>
      </locale>
    </schema>

    <schema><!--tab:core-->
      <key>/schemas/apps/darktable/parallel_export</key>
      <applyto>/apps/darktable/parallel_export</applyto>
      <owner>darktable</owner>
      <type>int</type>
      <default>1</default>
      <gettext_domain>darktable</gettext_domain>
      <locale name="C">
        <short>export multiple images in parallel</short>
        <long>set this variable to num_threads if you want multithreaded export to process multiple images at a time. be warned: every thread will need about 1GB of memory. setting this to 1 switches on per-image parallelization.</long>
      </locale>
    </schema>

    <schema><!--tab:core-->
      <key>/schemas/apps/darktable/host_memory_limit</key>
      <applyto>/apps/darktable/host_memory_limit</applyto>
      <owner>darktable</owner>
      <type>int</type>
      <default>1500</default>
      <gettext_domain>darktable</gettext_domain>
      <locale name="C">
        <short>host memory limit (in MB) for tiling</short>
        <long>this variable controls the maximum amount of memory (in MB) a module may use during image processing. lower values will force memory hungry modules to process image with increasing number of tiles. setting this to 0 will omit any limit. values below 500 will be treated as 500. needs a restart.</long>
      </locale>
    </schema>

    <schema><!--tab:core-->
      <key>/schemas/apps/darktable/singlebuffer_limit</key>
      <applyto>/apps/darktable/singlebuffer_limit</applyto>
      <owner>darktable</owner>
      <type>int</type>
      <default>16</default>
      <gettext_domain>darktable</gettext_domain>
      <locale name="C">
        <short>minimum amount of memory (in MB) for a single buffer in tiling</short>
        <long>if set to a positive, non-zero value this variable defines the minimum amount of memory (in MB) that tiling should take for a single image buffer. has precedence over heuristics based on host_memory_limit. needs a restart.</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/darktable/opencl_memory_headroom</key>
      <applyto>/apps/darktable/opencl_memory_headroom</applyto>
      <owner>darktable</owner>
      <type>int</type>
      <default>300</default>
      <gettext_domain>darktable</gettext_domain>
      <locale name="C">
        <short>amount of opencl memory (in MB) which we assume as being reserved for the driver</short>
        <long>this amount of memory (in MB) will be substracted from total gpu memory in order to calculate the available opencl memory. too low values will lead to out-of-memory situations in opencl processing. too high values will lead to unnecessary tiling (needs a restart).</long>
      </locale>
    </schema>


    <schema><!--tab:gui-->
      <key>/schemas/apps/darktable/never_use_embedded_thumb</key>
      <applyto>/apps/darktable/never_use_embedded_thumb</applyto>
      <owner>darktable</owner>
      <type>bool</type>
      <default>false</default>
      <gettext_domain>darktable</gettext_domain>
      <locale name="C">
        <short>don't use embedded preview jpeg but half-size raw</short>
        <long>check this option to not use the embedded jpeg from the raw file but process the raw data. this is slower but gives you color managed thumbnails.</long>
      </locale>
    </schema>

    <schema><!--tab:core-->
      <key>/schemas/apps/darktable/write_sidecar_files</key>
      <applyto>/apps/darktable/write_sidecar_files</applyto>
      <owner>darktable</owner>
      <type>bool</type>
      <default>true</default>
      <gettext_domain>darktable</gettext_domain>
      <locale name="C">
        <short>write sidecar file for each image</short>
        <long>these redundant files can later be re-imported into a different database, preserving your changes to the image.</long>
      </locale>
    </schema>

    <schema><!--tab:core-->
      <key>/schemas/apps/darktable/opencl</key>
      <applyto>/apps/darktable/opencl</applyto>
      <owner>darktable</owner>
      <type>bool</type>
      <default>flase</default>
      <gettext_domain>darktable</gettext_domain>
      <locale name="C">
        <short>activate opencl support (experimental)</short>
        <long>if found, use opencl runtime on your system for improved processing speed. this code is still experimental and not complete. please report crashes to the mailinglist darktable-devel at lists.sourceforge.net</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/darktable/opencl_library</key>
      <applyto>/apps/darktable/opencl_runtime</applyto>
      <owner>darktable</owner>
      <type>string</type>
      <default></default>
      <gettext_domain>darktable</gettext_domain>
      <locale name="C">
        <short>system library with opencl runtime</short>
        <long>opencl runtime library is normally detected automatically by darktable. if your opencl runtime is at an unusual place and cannot be detected, enter the full pathname here. leave empty for default behavior.</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/darktable/opencl_memory_requirement</key>
      <applyto>/apps/darktable/opencl_memory_requirement</applyto>
      <owner>darktable</owner>
      <type>int</type>
      <default>768</default>
      <gettext_domain>darktable</gettext_domain>
      <locale name="C">
        <short>minmum amount of GPU memory (MB) required to activate opencl</short>
        <long>opencl will only be activated if your graphics card has at least this amount of memory. reducing the value will allow cards with less GPU memory to be used - but at the risk of lower system stability and occasional crashes. values below 256 will be treated as 256.</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/darktable/opencl_size_roundup</key>
      <applyto>/apps/darktable/opencl_size_roundup</applyto>
      <owner>darktable</owner>
      <type>int</type>
      <default>16</default>
      <gettext_domain>darktable</gettext_domain>
      <locale name="C">
        <short>round opencl work group sizes to a multiple of</short>
        <long>in opencl processing round width/height of global work groups to a multiple of this value. reasonable values are powers of 2. this parameter can have high impact on opencl performance.</long>
      </locale>
    </schema>

    <schema><!--tab:gui-->
      <key>/schemas/apps/darktable/ask_before_remove</key>
      <applyto>/apps/darktable/ask_before_remove</applyto>
      <owner>darktable</owner>
      <type>bool</type>
      <default>true</default>
      <gettext_domain>darktable</gettext_domain>
      <locale name="C">
        <short>ask before removing images from database</short>
        <long>always ask the user before any image is removed from db.</long>
      </locale>
    </schema>

    <schema><!--tab:gui-->
      <key>/schemas/apps/darktable/ask_before_delete</key>
      <applyto>/apps/darktable/ask_before_delete</applyto>
      <owner>darktable</owner>
      <type>bool</type>
      <default>true</default>
      <gettext_domain>darktable</gettext_domain>
      <locale name="C">
        <short>ask before erasing images from disk</short>
        <long>always ask the user before any image file is deleted.</long>
      </locale>
    </schema>

    <schema><!--tab:gui-->
