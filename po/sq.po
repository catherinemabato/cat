msgstr "qartësoj shpejt"

#: ../src/iop/diffuse.c:482
msgid "fast local contrast"
msgstr "kontrast lokal i shpejtë"

#: ../src/iop/diffuse.c:1110 ../src/iop/diffuse.c:1363
