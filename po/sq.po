msgid "(%)"
msgstr "(%)"

#. Page SCENE
#: ../src/iop/filmicrgb.c:3682 ../src/iop/filmicrgb.c:4161
msgid "scene"
msgstr "skena"

#. axis legend
#: ../src/iop/filmicrgb.c:3691
