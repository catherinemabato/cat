#: ../src/iop/negadoctor.c:915
msgid "Illuminant red gain"
msgstr "光源紅光增益"

#: ../src/iop/negadoctor.c:916 ../src/iop/negadoctor.c:923
#: ../src/iop/negadoctor.c:930
msgid ""
