msgid "Perceptual"
msgstr "感知"

#: ../src/iop/colorout.c:859 ../src/libs/export.c:1266
#: ../src/libs/print_settings.c:2326 ../src/libs/print_settings.c:2647
#: ../src/views/darkroom.c:2471 ../src/views/lighttable.c:1224
